{
  "name": "z1lab/laravel-meta-tags",
  "keywords": [
    "meta",
    "tags",
    "head",
    "laravel",
    "open graph",
    "twitter card"
  ],
  "description": "A package to manage Header Meta Tags in Laravel projects",
  "license": "MIT",
  "authors": [
    {
      "name": "Lito",
      "email": "lito@eordes.com"
    },
    {
      "name": "Olimar Ferraz",
      "email": "webmaster@z1lab.com.br"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "~5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "Z1lab\\LaravelMeta\\": "src/Z1lab/LaravelMeta/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Z1lab\\LaravelMeta\\MetaServiceProvider"
      ],
      "aliases": {
        "Meta": "Z1lab\\LaravelMeta\\Facade"
      }
    }
  }
}
